<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
 <hibernate-mapping>
 <class name="beans.Movie" table="movie">
 	<id name="mid"></id>
 	<property name="mname"></property>
 	<many-to-one name="actor" class="beans.Actor" cascade="all">
 		<column name="aid"></column>
 	</many-to-one>
  </class>
 </hibernate-mapping>